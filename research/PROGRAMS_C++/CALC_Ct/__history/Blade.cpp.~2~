//---------------------------------------------------------------------------


#pragma hdrstop

#include "Blade.h"


__fastcall  TBlade::TBlade()
{
  // мощность на передачу
	 mPowerPrd = 4000.;
	// КУ на передачу
	 mKYPrd = 3000.;
}
// Конструктор копирования
__fastcall  TBlade::TBlade (const TBlade &R2)
 {
	 mPowerPrd = R2.mPowerPrd ;
	 mKYPrd  = R2.mKYPrd;
 }
 // оператор присваивания
  TBlade TBlade::operator=(TBlade  R2)
 {
	 mPowerPrd = R2.mPowerPrd ;
	 mKYPrd  = R2.mKYPrd;
	 return *this ;
 }

 // парам констр 1
 __fastcall TBlade::TBlade(const double PowerPrd,const double KYPrd)
 {
	 mPowerPrd = PowerPrd;
	 mKYPrd = KYPrd;
 }

#pragma package(smart_init)
