//---------------------------------------------------------------------------

#ifndef BladeH
#define BladeH
class TBlade
{
public:

 // мощность на передачу
	double mPowerPrd;
	// КУ на передачу
	double mKYPrd;




 __fastcall  TBlade() ;
// Конструктор копирования
__fastcall  TBlade (const TBlade &R2) ;

 // оператор присваивания
 TBlade   operator=(TBlade  R2) ;

  // парам констр
 __fastcall TBlade(const double PowerPrd,const double KYPrd);





};
#endif
