  TComp cmpK2(valK21, valK22);

  TComp cmpA = cmpK1 * cmp_F1 + cmpK2 * cmp_F2 -  cmpSZv;
//  TComp cmpA2 = valK12* valF1 + valK22 * valF2 -  cmpSZv.m_Im ;

