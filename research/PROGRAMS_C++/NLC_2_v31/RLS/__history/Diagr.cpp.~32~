  TComp cmp_dF1  = dF_po_dTet (alfTrg);
  TComp cmp_d2F1 = d2F_po_dTet2 (alfTrg);

  TComp cmp_F2   = fncF (alfAnp);
  TComp cmp_dF2  = dF_po_dTet (alfAnp);
  TComp cmp_d2F2 = d2F_po_dTet2 (alfAnp);

