//---------------------------------------------------------------------------


#pragma hdrstop

#include "Helic.h"


__fastcall  THelic::THelic()
{
  // мощность на передачу
	 mPowerPrd = 4000.;
	// КУ на передачу
	 mKYPrd = 3000.;
}
// Конструктор копирования
__fastcall  THelic::THelic (const THelic &R2)
 {
	 mPowerPrd = R2.mPowerPrd ;
	 mKYPrd  = R2.mKYPrd;
 }
 // оператор присваивания
  THelic THelic::operator=(THelic  R2)
 {
	 mPowerPrd = R2.mPowerPrd ;
	 mKYPrd  = R2.mKYPrd;
	 return *this ;
 }

 // парам констр 1
 __fastcall THelic::THelic(const double PowerPrd,const double KYPrd)
 {
	 mPowerPrd = PowerPrd;
	 mKYPrd = KYPrd;
 }

#pragma package(smart_init)
